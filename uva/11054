#include<bits/stdc++.h>
using namespace std;
#define MOD 1000000007
typedef long long ll;


int main()
{

    ll n;
    while(cin>>n && n){

        ll a[n+5];

        for(int i=0;i<n;i++){
            cin>>a[i];
        }

        ll ans1=0,ans2=0;
        ll total=0;

        for(int i=0;i<n;i++){
            total+=a[i];
            ans1+=abs(total);
        }
        cout<<ans1<<endl;

    }
}
