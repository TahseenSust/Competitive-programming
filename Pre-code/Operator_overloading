struct data{
  int x,w;
  data(){}
  data(int a,int b){
    x = a;
    w = b;
  }
  bool operator <(const data &yoo)const{
    return yoo.w < w;
  }
};

struct data{
    int u,v;
    ll w;
    data(int u,int v,ll w):u(u),v(v),w(w){}
    bool friend operator <(data A,data B){
        return A.w<B.w;
    }
};
